{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Host": "http://localhost:5000",
  "Polly": {
    "CircuitBreaker": {
      "DurationOfBreak": "00:00:30",
      "FailureThreshold": 1,
      "MinimumThroughput": 10
    },
    "Retry": {
      "Count": 3,
      "Delay": "00:00:05"
    },
    "Timeout": {
      "Duration": "00:00:10"
    },
    "Bulkhead": {
      "MaxParallelization": 5,
      "MaxQueuingActions": 10
    }
  }
}
