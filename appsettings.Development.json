{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Polly": {
    "CircuitBreaker": {
      "DurationOfBreak": "00:00:30",
      "FailureThreshold": 5,
      "MinimumThroughput": 10
    },
    "Retry": {
      "Count": 10,
      "Delay": "00:00:15"
    },
    "Timeout": {
      "Duration": "00:02:00"
    },
    "Bulkhead": {
      "MaxParallelization": 1,
      "MaxQueuingActions": 1
    }
  }
}
